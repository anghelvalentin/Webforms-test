for (int i = 1; i <= 100; i++)
{
    if (i % 3 == 0 || i % 5 == 0)
    {
        if (i % 3 == 0 && i % 5 == 0)
        {
            Console.Write("ING BANK ");
        }
        else if (i % 3 == 0)
        {
            Console.Write("ING ");
        }
        else
        {
            Console.Write("Bank ");
        }
    }
    else
    {
        Console.Write(i + " ");
    }
}
